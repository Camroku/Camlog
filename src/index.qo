#!/usr/local/bin/qo
include qo;
include webutil as w;

route = deleteAfter(qo.env("REQUEST_URI"), '?');

if (route == "/")
{
    w.htmlPage("mainpage");
}
else if (route == "/something.qo")
{
    username = "Katroku";
    content = format(read("index.html"));
    w.htmlPage(content);
}
else
{
    w.notFound();
};
